{
    "compilerOptions": {
        "baseUrl": ".",
        "composite": true,
        "noFallthroughCasesInSwitch": true,
        "module": "CommonJS",
        "outDir": "dist",
        "rootDir": ".",
        "noEmit": false,
        "downlevelIteration": true,
        "target": "ES2018",
        "strict": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "noImplicitReturns": true,
        "declaration": true,
        "skipLibCheck": true,
        "allowJs": true,
        "checkJs": false,
        "esModuleInterop": true
    }
}
