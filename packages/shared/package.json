    "styled-components": "^5.3.5",
    "styled-system": "^5.1.5",
    "tinycolor2": "^1.4.2",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0"
